<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewcontroller.RelatorioTransporteVivoController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="349.0" layoutY="14.0" text="Transporte Carga Viva">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Button layoutX="1006.0" layoutY="680.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="60.0" text="Sair" fx:id="buttonSair" onAction="#handleSair" />
            <TableView layoutX="14.0" layoutY="66.0" prefHeight="600.0" prefWidth="1045.0" fx:id="tabelaTransporteVivo">
               <columns>
                  <TableColumn prefWidth="75.0" text="Codigo" fx:id="codigoColumn" />
                  <TableColumn prefWidth="75.0" text="Nome" fx:id="nomeColumn" />
                  <TableColumn prefWidth="75.0" text="Descriçao" fx:id="descricaoColumn" />
                  <TableColumn prefWidth="75.0" text="Peso (kg)" fx:id="pesoColumn"/>
                  <TableColumn prefWidth="75.0" text="Distancia (km)" fx:id="distanciaColumn"/>
                  <TableColumn prefWidth="75.0" text="Climatizado" fx:id="climatizadoColumn"/>
                  <TableColumn prefWidth="75.0" text="Temp. Min. (ºC)" fx:id="tempMinColumn"/>
                  <TableColumn prefWidth="75.0" text="Temp. Max. (ºC)" fx:id="tempMaxColumn"/>
                  <TableColumn prefWidth="75.0" text="Situaçao" fx:id="situacaoColumn"/>
                  <TableColumn prefWidth="75.0" text="Custo" fx:id="custoColumn"/>
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </AnchorPane>
   </children>
</VBox>
