            <Label layoutX="15.0" layoutY="181.0" text="Longitude de Origem" />
            <TextField fx:id="longitudeOField" editable="false" layoutX="149.0" layoutY="177.0" prefHeight="25.0" prefWidth="160.0" promptText="Longitude da origem" />
            <Label layoutX="330.0" layoutY="183.0" text="Longitude de Destino" />
            <TextField fx:id="longitudeDField" editable="false" layoutX="467.0" layoutY="177.0" prefHeight="25.0" prefWidth="160.0" promptText="Longitude do destino" />
