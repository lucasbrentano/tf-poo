<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewcontroller.RelatorioTransportePessoalController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="378.0" layoutY="14.0" text="Transporte Pessoal">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Button fx:id="sairButton" layoutX="1006.0" layoutY="680.0" mnemonicParsing="false" onAction="#handleSair" prefHeight="26.0" prefWidth="60.0" text="Sair" />
            <TableView fx:id="tabelaTransportePessoal" layoutX="14.0" layoutY="66.0" prefHeight="600.0" prefWidth="1045.0">
              <columns>
                <TableColumn fx:id="codigoTColumn" prefWidth="75.0" text="Codigo" />
                <TableColumn fx:id="nomeTColumn" prefWidth="75.0" text="Nome" />
                  <TableColumn fx:id="descricaoTColumn" prefWidth="75.0" text="Descriçao" />
                  <TableColumn fx:id="pesoTColumn" prefWidth="75.0" text="Peso (kg)" />
                  <TableColumn fx:id="distanciaTColumn" prefWidth="75.0" text="Distancia (km)" />
                  <TableColumn fx:id="passageirosTColumn" prefWidth="75.0" text="Passageiros" />
                  <TableColumn fx:id="situacaoTColumn" prefWidth="75.0" text="Situaçao" />
                  <TableColumn fx:id="custoTColumn" prefWidth="75.0" text="Custo" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </AnchorPane>
   </children>
</VBox>
